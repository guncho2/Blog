<h1>Create new Articles with Forms</h1>

<%= form_for :article, url: articles_path do |f| %>

    // added url: article_path to submit to the path of article//



<p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
</p>


 <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
</p>

<p>
    <%= f.submit %>
</p>

<% end %>




